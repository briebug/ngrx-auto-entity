{
  "name": "ngrx-auto-entity-app",
  "version": "0.0.0",
  "licnese": "MIT",
  "author": {
    "name": "BrieBug",
    "url": "https://briebug.com"
  },
  "contributors": [
    {
      "name": "Jon Rista",
      "url": "https://github.com/jrista"
    },
    {
      "name": "Kevin Schuchard",
      "url": "https://github.com/schuchard"
    }
  ],
  "scripts": {
    "ng": "ng",
    "start": "concurrently --prefix-colors white.bgBlue,white.bgRed --names angular,json-server --kill-others \"npm run serve-proxy\" \"npm run json-server\"",
    "serve": "ng serve",
    "serve:prod": "ng serve --configuration production",
    "serve:test": "ng serve test-app",
    "serve:test:prod": "ng serve test-app",
    "serve-proxy": "ng serve --configuration production --proxy-config proxy.conf.json",
    "serve-proxy:test": "ng serve test-app --proxy-config proxy.conf.json",
    "serve-proxy:test:prod": "ng serve test-app --configuration production --proxy-config proxy.conf.json",
    "build": "ng build --configuration production",
    "build:watch": "ng build --configuration production --watch",
    "build:app": "ng build ngrx-auto-entity-app --configuration production",
    "build:app:watch": "ng build ngrx-auto-entity-app --configuration production --watch",
    "build:lib": "ng build ngrx-auto-entity --configuration production",
    "build:lib:watch": "ng build ngrx-auto-entity --configuration production --watch",
    "build:ci": "ng build ngrx-auto-entity --configuration production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --runInBand --testResultsProcessor='jest-junit'",
    "lint": "ng lint ngrx-auto-entity",
    "lint:fix": "ng lint ngrx-auto-entity --fix",
    "json-server": "json-server --watch data/db.json --routes data/routes.json",
    "link": "ng build ngrx-auto-entity && npm link dist/ngrx-auto-entity",
    "link:ci": "ng build ngrx-auto-entity && sudo npm link dist/ngrx-auto-entity",
    "postinstall": "if test \"$NODE_ENV\" = \"ci\" ; then npm run link:ci ; else npm run link ; fi",
    "prettier": "prettier --write \"./**/*.ts\""
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "^12.1.3",
    "@angular/animations": "^12.1.3",
    "@angular/cdk": "^12.1.3",
    "@angular/common": "^12.1.3",
    "@angular/compiler": "^12.1.3",
    "@angular/core": "^12.1.3",
    "@angular/flex-layout": "12.0.0-beta.34",
    "@angular/forms": "^12.1.3",
    "@angular/material": "^12.1.3",
    "@angular/platform-browser": "^12.1.3",
    "@angular/platform-browser-dynamic": "^12.1.3",
    "@angular/router": "^12.1.3",
    "@briebug/ngrx-auto-entity": "file:./projects/ngrx-auto-entity",
    "@ngrx/effects": "^12.3.0",
    "@ngrx/router-store": "^12.3.0",
    "@ngrx/store": "^12.3.0",
    "core-js": "^2.5.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "6.6.7",
    "uuidv4": "^5.0.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.3",
    "@angular/cli": "^12.1.3",
    "@angular/compiler-cli": "^12.1.3",
    "@angular/language-service": "^12.1.3",
    "@ngrx/store-devtools": "^12.3.0",
    "@types/jest": "^24.9.1",
    "@types/node": "~12.0.12",
    "acorn": "6.4.1",
    "codelyzer": "^6.0.0",
    "concurrently": "^3.6.1",
    "faker": "^4.1.0",
    "husky": "^0.14.3",
    "jasmine-marbles": "^0.5.0",
    "jest": "^24.9.0",
    "jest-extended": "^0.11.2",
    "jest-junit": "^7.0.0",
    "jest-preset-angular": "^7.1.1",
    "json-server": "^0.15.0",
    "lint-staged": "^7.2.0",
    "ng-packagr": "^12.1.2",
    "ngrx-store-freeze": "^0.2.4",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.0",
    "ts-jest": "^24.0.2",
    "ts-node": "~5.0.1",
    "tsickle": "^0.39.1",
    "tslib": "^2.0.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "~4.3.5"
  },
  "lint-staged": {
    "src/app/**/*.ts": [
      "tslint --config tslint.json --project tsconfig.json --fix",
      "git add"
    ],
    "projects/ngrx-auto-entity/src/lib/**/*.ts": [
      "tslint --config tslint.json --project tsconfig.json --fix",
      "git add"
    ]
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/setup-jest.ts",
    "testMatch": [
      "**/+(*.)+(spec|test).+(ts)?(x)"
    ],
    "coverageReporters": [
      "html",
      "text"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.module.ts",
      "!src/main.ts",
      "!src/polyfills.ts",
      "!src/environments/*.ts",
      "projects/ngrx-auto-entity/src/lib/**/*.ts",
      "!projects/ngrx-auto-entity/src/lib/**/*.module.ts",
      "!projects/ngrx-auto-entity/src/lib/index.ts",
      "!<rootDir>/setup-jest.ts",
      "!<rootDir>/jestGlobalMocks.ts"
    ],
    "moduleNameMapper": {
      "^ngrx-auto-entity/(.*)": "<rootDir>/projects/ngrx-auto-entity/$1",
      "^state/(.*)": "<rootDir>/src/app/state/$1",
      "^core/(.*)": "<rootDir>/src/app/core/$1",
      "^models/(.*)": "<rootDir>/src/app/models/$1"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/dist"
    ]
  }
}
